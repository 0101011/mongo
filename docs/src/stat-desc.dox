@row{WT_STAT_file_col_fix_pages,column-store fixed-size leaf pages}
@row{WT_STAT_file_col_int_pages,column-store internal pages}
@row{WT_STAT_file_col_var_pages,column-store variable-size leaf pages}
@row{WT_STAT_cursor_first,cursor-first}
@row{WT_STAT_cursor_inserts,cursor-inserts}
@row{WT_STAT_cursor_last,cursor-last}
@row{WT_STAT_cursor_read,cursor-read}
@row{WT_STAT_cursor_read_near,cursor-read-near}
