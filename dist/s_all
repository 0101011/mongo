#! /bin/sh
#
# $Id$

# Run standard scripts.

trap 'rm -f __tmp_api dist.pyc; exit 0' 0 1 2 3 13 15

python api_defines.py			# Build API flags include file
python api.py				# Build API

sh ./s_prototypes			# Build prototypes include file
sh ./s_readme				# Build README file
sh ./s_tags				# Build tags files
