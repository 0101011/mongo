@row{WT_STAT_cursor_read_prev,cursor-read-prev}
@row{WT_STAT_cursor_removes,cursor-removes}
@row{WT_STAT_cursor_updates,cursor-updates}
@row{WT_STAT_alloc,file: block allocations}
@row{WT_STAT_extend,file: block allocations required file extension}
@row{WT_STAT_free,file: block frees}
@row{WT_STAT_overflow_read,file: overflow pages read from the file}
@row{WT_STAT_page_read,file: pages read from the file}
@row{WT_STAT_page_write,file: pages written to the file}
@row{WT_STAT_file_size,file: size}
@row{WT_STAT_file_fixed_len,fixed-record size}
@row{WT_STAT_file_magic,magic number}
@row{WT_STAT_file_major,major version number}
@row{WT_STAT_file_maxintlitem,maximum internal page item size}
@row{WT_STAT_file_maxintlpage,maximum internal page size}
@row{WT_STAT_file_maxleafitem,maximum leaf page item size}
