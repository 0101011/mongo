#! /bin/sh

# Build a list of internal function prototypes.
t=/tmp/__configure.$$
trap 'rm -f $t; exit 0' 0 1 2 3 13 15

for i in `sed -e '/^[a-z]/! d' filelist`; do
	sed -n \
	    -e '/^__wt_[a-z]/!{' \
		-e h \
		-e d \
	    -e '}' \
	    -e x \
	    -e '/^static/d' \
	    -e x \
	    -e ': loop' \
	    -e H \
	    -e n \
	    -e '/^{/!b loop' \
	    -e x \
	    -e 's/$/;/p' \
	    < ../$i
done > $t

f=../inc_posix/extern.h
cmp $t $f > /dev/null 2>&1 ||
    (echo "Building $f" && rm -f $f && cp $t $f && chmod 444 $f)
